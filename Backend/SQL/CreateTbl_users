CREATE TABLE  `0fe_17505155_scu`.`tbl_donaters` (
`UID` INT( 50 ) NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT  'User ID',
`EMAIL` VARCHAR( 50 ) NOT NULL ,
`NAME` VARCHAR( 50 ) NOT NULL ,
`PHONE` INT( 10 ) NOT NULL ,
`PASSWORD` VARCHAR( 12 ) NOT NULL
) ENGINE = MYISAM COMMENT =  'Used for registration and login';


CREATE TABLE  `0fe_17505155_scu`.`tel_recv` (
`PHONE` INT NOT NULL ,
`NAME` VARCHAR( 50 ) NOT NULL ,
`ITEM` VARCHAR( 100 ) NOT NULL ,
`LOCATION` INT( 6 ) NOT NULL
) ENGINE = MYISAM ;